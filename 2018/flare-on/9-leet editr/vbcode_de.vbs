                oIe.document.parentwindow.execScript(code)
                ascii = oIe.document.getElementById("textin").value
                Exit Do
            End If
            happy = PetMeLikeATurtle()
        Loop
    End If

    title = ""
    still_running = True
    Do While still_running And Not happy
